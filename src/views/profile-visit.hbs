<div class="card text-center m-4">
    <div class="card-header bg-dark">

        <h5 class="text-white">Estas en el perfil de {{user.user}}</h5>
    </div>
    <div class="card-body">
        <div class="row">
            {{#each images}}
            <div class="card col-md-4 mb-2">
                <div class="card-header">
                    <h5>{{title}}</h5>
                </div>
                <div class="card-body">

                <a href="/photo/{{_id}}">
                    <img src="{{url}}" alt="" class="w-100 h-100">
                </a>
                </div>
            </div>
            {{/each}}
        </div>
    </div>
</div>